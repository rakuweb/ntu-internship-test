{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:proxy": "next dev & local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "build": "next build",
    "start": "next start -p 3003",
    "start:test": "next start -p 3001",
    "lint": "next lint",
    "export": "next build && next export",
    "postbuild": "next-sitemap",
    "test": "jest",
    "format": "prettier --write ./src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gen:theme-typings": "chakra-cli tokens ./src/lib/theme/index.ts",
    "gen:graphql": "graphql-codegen --config codegen.yml",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@chakra-ui/react": "^2.4.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@hookform/resolvers": "^2.8.3",
    "@line/liff": "^2.21.3",
    "@line/liff-mock": "^1.0.3",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.10.14",
    "antd": "^5.0.2",
    "axios": "^1.2.0",
    "framer-motion": "^7.6.18",
    "graphql": "^16.6.0",
    "next": "13.0.6",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.20.5",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^9.4.1",
    "recoil": "^0.7.6",
    "swiper": "8.4.5",
    "three": "0.147.0",
    "use-sound": "^4.0.1",
    "yarn": "^1.22.17",
    "yup": "^0.32.11",
    "zenn-content-css": "^0.1.141",
    "zenn-markdown-html": "^0.1.141",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/client-preset": "1.2.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@liff/liff-types": "^2.22.0",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@svgr/webpack": "^7.0.0",
    "@types/gtag.js": "^0.0.12",
    "@types/react": "^18.0.26",
    "@types/three": "^0.146.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-loader": "^9.1.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.7",
    "file-loader": "^6.2.0",
    "firebase-tools": "^11.24.0",
    "hygen": "^6.1.0",
    "lint-staged": "^13.0.4",
    "local-ssl-proxy": "^2.0.5",
    "next-sitemap": "^3.1.32",
    "prettier": "^2.3.2",
    "storybook-addon-next-router": "^4.0.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint .",
      "yarn format"
    ]
  }
}
