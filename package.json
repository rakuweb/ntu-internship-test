    "@mui/material": "^5.10.14",
    "@next/font": "^14.2.1",
    "antd": "^5.0.2",
    "axios": "^1.2.0",
    "chakra-react-select": "^4.6.0",
    "framer-motion": "^7.6.18",
    "graphql": "^16.6.0",
    "next": "13.5.1",
    "next-seo": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha-v3": "^1.10.1",
