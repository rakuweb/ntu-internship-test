{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "dev": "next dev",
    "dev:proxy": "next dev & local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "build": "next build",
    "start": "next start -p 3003",
    "start:test": "next start -p 3001",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "export": "next build && next export",
    "postbuild": "next-sitemap",
    "test": "jest",
    "format": "prettier --write ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storycap": "storycap --serverCmd \"npx http-server storybook-static -p 6006\" http://localhost:6006",
    "gen:theme-typings": "chakra-cli tokens ./src/lib/theme/index.ts",
    "gen:graphql": "graphql-codegen --config codegen.yml",
    "codegen": "graphql-codegen --config codegen.ts",
    "offer-codegen": "graphql-codegen --config offercodegen.ts"
  },
  "dependencies": {
